{
    "name": "egg-party",
    "version": "1.0.0",
    "scripts": {
        "build": "rimraf egg-party-api && tsc && copyfiles src/function.json egg-party-api --flat",
        "migrate:gen": "typeorm migration:generate -c Local && npm run build",
        "migrate:run": "typeorm migration:run -c Local",
        "migrate:run:prod": "typeorm migration:run -c Production"
    },
    "description": "Egg Party",
    "main": "main.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cakekindel/egg-party.git"
    },
    "keywords": [
        "egg",
        "party",
        "slack",
        "app",
        "plugin"
    ],
    "author": "Orion Kindel",
    "license": "ISC",
    "dependencies": {
        "@azure/functions": "^1.0.3",
        "@nestjs/azure-func-http": "^0.1.2",
        "@nestjs/common": "^6.0.0",
        "@nestjs/core": "^6.0.0",
        "@nestjs/microservices": "^6.5.3",
        "@nestjs/platform-express": "^6.5.3",
        "@nestjs/websockets": "^6.5.3",
        "@wufe/mapper": "^0.5.0",
        "automapper-ts": "^1.9.0",
        "axios": "^0.19.0",
        "copyfiles": "^2.1.1",
        "hash.js": "^1.1.7",
        "morphism": "^1.12.0",
        "mssql": "^5.1.0",

{
    "name": "egg-party",
    "version": "1.0.0",
    "scripts": {
        "start": "npm run serve",
        "test": "npm run test:single-run",
        "\n -- Serve --": "",
        "serve": "npm run clean && concurrently npm:serve:watch npm:serve:start -k -c \"cyan.dim,green.dim\" -n \" build ,  api  \"",
        "\n -- Test --": "",
        "test:single-run": "mocha",
        "test:single-run-debug": "mocha --inspect-brk=\"9231\"",
        "test:watch": "concurrently npm:test:watch-test npm:test:watch-src -k -c \"cyan.dim,green.dim\"  -n \" mocha , build \"",
        "test:junit": "mocha --opts test/mocha.junit.opts",
        "\n -- Database --": "",
        "migration:generate": "typeorm migration:generate -d ./src/db/migrations",
        "migration:run": "typeorm migration:run",
        "\n -- Utilities --": "",
        "tunnel": "ngrok http 7000 --host-header=\"localhost:7000\"",
        "\n -- Docker --": "",
        "docker:build": "docker build -t cakekindel/egg-party .",
        "docker:run": "docker run -p 8282:8080 cakekindel/egg-party",
        "\n -- Build --": "",
        "build": "tsc --build",
        "build:prod": "tsc --build ./src/tsconfig.prod.json",
        "\n -- Clean --": "",
        "clean": "rimraf dist",
        "\n -- Scripts Used by Other Scripts (don't invoke directly) --": "",
        "prebuild": "npm run clean",
        "premigration:generate": "npm run build",
        "premigration:run": "npm run build",
        "serve:watch": "webpack --config ./webpack.watch.config.js --info-verbosity verbose",
        "serve:start": "node --inspect=\"127.0.0.1:9230\" ./dist/main.js",
        "preserve:start": "wait-on ./dist/main.js",
        "test:watch-test": "mocha-typescript-watch -p ./test/tsconfig.json -o ./test/mocha.ts-watch.opts",
        "test:watch-src": "tsc --build src --watch & mocha-typescript-watch -p ./test/tsconfig.json -o ./test/mocha.ts-watch.opts",
        "pretest:watch": "npm run clean",
        "pretest:junit": "npm run build",
        "pretest:single-run": "npm run build",
        "pretest:single-run-debug": "npm run build"
    },
    "description": "Egg Party",
    "main": "dist/main.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cakekindel/egg-party.git"
    },
    "keywords": [
        "egg",
        "party",
        "slack",
        "app",
        "plugin"
    ],
    "author": "Orion Kindel",
    "license": "ISC",
    "dependencies": {
        "@azure/functions": "^1.0.3",
        "@nestjs/azure-func-http": "^0.3.1",
        "@nestjs/common": "^6.8.4",
        "@nestjs/core": "^6.8.4",
        "@nestjs/microservices": "^6.8.4",
        "@nestjs/platform-express": "^6.8.4",
        "@nestjs/websockets": "^6.8.4",
        "@types/sinon": "^7.5.0",
        "@wufe/mapper": "^0.5.0",
        "automapper-ts": "^1.9.0",
        "axios": "^0.19.0",
        "copyfiles": "^2.1.1",
        "dotenv": "^8.2.0",
        "hash.js": "^1.1.7",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "morphism": "^1.12.1",
        "mssql": "^6.0.1",
        "qs": "^6.9.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.3",
        "sinon": "^7.5.0",
        "typeorm": "^0.2.20"
    },
    "devDependencies": {
        "@fluffy-spoon/substitute": "^1.89.0",
        "@nestjs/testing": "^6.8.4",
        "@types/axios": "^0.14.0",
        "@types/chai": "^4.2.4",
        "@types/express": "4.17.2",
        "@types/lodash": "^4.14.149",
        "@types/mocha": "^5.2.7",
        "@types/node": "12.12.14",
        "@types/qs": "^6.5.3",
        "@types/supertest": "2.0.8",
        "chai": "^4.2.0",
        "concurrently": "^5.0.0",
        "jest": "24.9.0",
        "mocha": "^6.2.2",
        "mocha-junit-reporter": "^1.23.1",
        "mocha-typescript": "^1.1.17",
        "ngrok": "^3.2.5",
        "prettier": "1.19.1",
        "supertest": "4.0.2",
        "ts-loader": "^6.2.1",
        "ts-node": "8.5.2",
        "tsc-watch": "4.0.0",
        "tsconfig-paths": "3.9.0",
        "tslint": "5.20.1",
        "typescript": "^3.7.2",
        "wait-on": "^3.3.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-node-externals": "^1.7.2"
    }
}

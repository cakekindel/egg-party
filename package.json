{
    "name": "egg-party",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run clean && tsc --build",
        "clean": "rimraf dist",
        "test": "npm run build && mocha",
        "test:watch": "mocha-typescript-watch -p ./test/tsconfig.json -o ./test/mocha.ts-watch.opts",
        "migrate:gen": "typeorm migration:generate -c Local && npm run build",
        "migrate:run": "typeorm migration:run -c Local"
    },
    "description": "Egg Party",
    "main": "main.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cakekindel/egg-party.git"
    },
    "keywords": [
        "egg",
        "party",
        "slack",
        "app",
        "plugin"
    ],
    "author": "Orion Kindel",
    "license": "ISC",
    "dependencies": {
        "@azure/functions": "^1.0.3",
        "@nestjs/azure-func-http": "^0.1.2",
        "@nestjs/common": "^6.8.4",
        "@nestjs/core": "^6.8.4",
        "@nestjs/microservices": "^6.8.4",
        "@nestjs/platform-express": "^6.8.4",
        "@nestjs/websockets": "^6.8.4",
        "@types/sinon": "^7.5.0",
        "@wufe/mapper": "^0.5.0",
        "automapper-ts": "^1.9.0",
        "axios": "^0.19.0",
        "copyfiles": "^2.1.1",
        "hash.js": "^1.1.7",
        "moment": "^2.24.0",
        "morphism": "^1.12.1",
        "mssql": "^5.1.0",
        "qs": "^6.9.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^2.7.1",
        "rxjs": "^6.5.3",
        "sinon": "^7.5.0",
        "typeorm": "^0.2.20"
    },
    "devDependencies": {
        "@fluffy-spoon/substitute": "^1.89.0",
        "@nestjs/testing": "^6.8.4",
        "@types/axios": "^0.14.0",
        "@types/chai": "^4.2.4",
        "@types/express": "4.16.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "11.13.4",
        "@types/qs": "^6.5.3",
        "@types/supertest": "2.0.7",
        "chai": "^4.2.0",
